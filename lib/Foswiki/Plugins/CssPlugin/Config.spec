# ---+ CssPlugin


# **STRING**
# Class name for button
$Foswiki::cfg{Plugins}{CssPlugin}{button} = "fw-button";

# **STRING**
# Class name for table. Think table element.
$Foswiki::cfg{Plugins}{CssPlugin}{table} = "fw-table";

# **STRING**
# Class name for table caption. Think caption element.
$Foswiki::cfg{Plugins}{CssPlugin}{caption} = "fw-caption";

# **STRING**
# Class name for table row. Think tr element.
$Foswiki::cfg{Plugins}{CssPlugin}{tr} = "fw-tr";

# **STRING**
# Class name for table data cell. Think td element.
$Foswiki::cfg{Plugins}{CssPlugin}{td} = "fw-td";

# **STRING**
# Class name for table header cell. Think th element.
$Foswiki::cfg{Plugins}{CssPlugin}{th} = "fw-th";

# **STRING**
# Class name for table header group. Think thead element.
$Foswiki::cfg{Plugins}{CssPlugin}{thead} = "fw-thead";

# **STRING**
# Class name for table row group. Think tbody element.
$Foswiki::cfg{Plugins}{CssPlugin}{tbody} = "fw-tbody";

# **STRING**
# Class name for table footer group.
$Foswiki::cfg{Plugins}{CssPlugin}{tfoot} = "fw-tfoot";

# **STRING**
# Class name for odd table row or any odd stuff.
$Foswiki::cfg{Plugins}{CssPlugin}{odd} = "fw-odd";









