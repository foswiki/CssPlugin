# ---+ CssPlugin

# **STRING**
# Class name for page body. Think body element.
$Foswiki::cfg{Plugins}{CssPlugin}{'body'} = 'ui-widget ui-widget-content';

# **STRING**
# Class name for page header. Think page header.
$Foswiki::cfg{Plugins}{CssPlugin}{'header'} = 'ui-widget-header';

# **STRING**
# Class name for page header. Think page footer.
$Foswiki::cfg{Plugins}{CssPlugin}{'footer'} = 'ui-widget-header';
	
# **STRING**
# Class name for button. Think widget.
$Foswiki::cfg{Plugins}{CssPlugin}{'button'} = 'fw-button ui-state-default ui-corner-all';

# **STRING**
# Class name for cancel button. Think widget.
$Foswiki::cfg{Plugins}{CssPlugin}{'button-cancel'} = 'fw-button ui-state-default ui-state-error ui-corner-all';

# **STRING**
# Class name for primary button. Think widget.
$Foswiki::cfg{Plugins}{CssPlugin}{'button-primary'} = 'fw-button ui-state-default ui-priority-primary ui-corner-all';

# **STRING**
# Class name for secondary button. Think widget.
$Foswiki::cfg{Plugins}{CssPlugin}{'button-secondary'} = 'fw-button ui-state-default ui-priority-secondary ui-corner-all';

# **STRING**
# Class name for topic data form. 
$Foswiki::cfg{Plugins}{CssPlugin}{'data-form'} = 'fw-data-form';

# **STRING**
# Class name for table. Think table element.
$Foswiki::cfg{Plugins}{CssPlugin}{'table'} = 'fw-table';

# **STRING**
# Class name for table caption. Think caption element.
$Foswiki::cfg{Plugins}{CssPlugin}{'caption'} = 'fw-caption ui-widget-header';

# **STRING**
# Class name for table row. Think tr element.
$Foswiki::cfg{Plugins}{CssPlugin}{'tr'} = 'fw-tr ui-widget-content';

# **STRING**
# Class name for table header cell. Think th element.
$Foswiki::cfg{Plugins}{CssPlugin}{'th'} = 'fw-th ui-widget-header';

# **STRING**
# Class name for table data cell. Think td element.
$Foswiki::cfg{Plugins}{CssPlugin}{'td'} = 'fw-td';

# **STRING**
# Class name for table header group. Think thead element.
$Foswiki::cfg{Plugins}{CssPlugin}{'thead'} = 'fw-thead';

# **STRING**
# Class name for table row group. Think tbody element.
$Foswiki::cfg{Plugins}{CssPlugin}{'tbody'} = 'fw-tbody';

# **STRING**
# Class name for table footer group.
$Foswiki::cfg{Plugins}{CssPlugin}{'tfoot'} = 'fw-tfoot';

# **STRING**
# Class name for odd table row or any odd stuff.
$Foswiki::cfg{Plugins}{CssPlugin}{'odd'} = 'fw-odd ui-widget-content ui-state-default';

# **STRING**
# Class name for even table row.
$Foswiki::cfg{Plugins}{CssPlugin}{'even'} = 'fw-even ui-widget-content ui-state-hover';

# **STRING**
# Class name for sorted items. Think sorted column in tables.
$Foswiki::cfg{Plugins}{CssPlugin}{'sorted'} = 'ui-state-highlight';

# **STRING**
# Class name for text area.
$Foswiki::cfg{Plugins}{CssPlugin}{'text-area'} = 'fw-input ui-widget-content';

# **STRING**
# Class name for text input.
$Foswiki::cfg{Plugins}{CssPlugin}{'text-input'} = 'fw-input ui-widget-content';

# **STRING**
# Class name for disabled control.
$Foswiki::cfg{Plugins}{CssPlugin}{'disabled'} = 'ui-state-disabled';

# **STRING**
# Class name for CSS clear.
$Foswiki::cfg{Plugins}{CssPlugin}{'clear'} = 'ui-helper-clear';

# **STRING**
# Class name for checkbox contrsol.
$Foswiki::cfg{Plugins}{CssPlugin}{'ckeckbox'} = 'fw-checkbox';

# **STRING**
# Class name for error text.
$Foswiki::cfg{Plugins}{CssPlugin}{'error-text'} = 'ui-state-error-text';

# **STRING**
# Class name for error display control.
$Foswiki::cfg{Plugins}{CssPlugin}{'error'} = 'fw-error ui-widget ui-state-error ui-corner-all';

# **STRING**
# Class name for primary priority control.
$Foswiki::cfg{Plugins}{CssPlugin}{'primary'} = 'ui-priority-primary';










